    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>6130877b-61a8-4083-8fe7-9ba0aded14de</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="6.0.15" />
